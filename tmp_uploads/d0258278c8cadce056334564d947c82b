(34, 'http://www.sportslab.com.tw/image/member/album/resize/623/629/1504596555497.png', '捷安特公路車', '1', '好車一台', '1000', '公路車', '全車', '捷安特', '藍色', '20吋'),
(41, 'http://www.sportslab.com.tw/image/member/album/resize/623/629/1504596555497.png', '捷安特公路車', '1231', '1231', '12313', '公路車', '握把.龍頭', '123131', '1231', '12313');

--
-- 已匯出資料表的索引
--

--
-- 資料表索引 `orders`
--
ALTER TABLE `orders`
  ADD PRIMARY KEY (`sid`),
  ADD UNIQUE KEY `sid` (`sid`);

--
-- 資料表索引 `prouduct`
--
ALTER TABLE `prouduct`
  ADD PRIMARY KEY (`p_sid`);

--
-- 資料表索引 `prouductcarousel`
--
ALTER TABLE `prouductcarousel`
  ADD PRIMARY KEY (`p_sid`);

--
-- 在匯出的資料表使用 AUTO_INCREMENT
--

--
-- 使用資料表 AUTO_INCREMENT `orders`
--
ALTER TABLE `orders`
  MODIFY `sid` int(255) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=51;

--
-- 使用資料表 AUTO_INCREMENT `prouduct`
--
ALTER TABLE `prouduct`
  MODIFY `p_sid` int(11) NOT NULL AUTO_INCREMENT COMMENT '商品流水號', AUTO_INCREMENT=148;

--
-- 使用資料表 AUTO_INCREMENT `prouductcarousel`
--
ALTER TABLE `prouductcarousel`
  MODIFY `p_sid` int(11) NOT NULL AUTO_INCREMENT COMMENT '商品流水號', AUTO_INCREMENT=42;
