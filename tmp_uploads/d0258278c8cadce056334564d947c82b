--
-- 資料表結構 `prouductcarousel`
--

CREATE TABLE `prouductcarousel` (
  `p_sid` int(11) NOT NULL COMMENT '商品流水號',
  `p_photo` varchar(255) NOT NULL COMMENT '商品照片',
  `p_name` varchar(255) NOT NULL COMMENT '商品名稱',
  `quantity` varchar(255) NOT NULL COMMENT '數量',
  `p_description` varchar(255) NOT NULL COMMENT '說明',
  `p_price` varchar(255) NOT NULL COMMENT '價格',
  `p_genre` varchar(255) NOT NULL COMMENT '商品類別',
  `p_genre2` varchar(255) NOT NULL COMMENT '商品部件',
  `p_brand` varchar(255) NOT NULL COMMENT '品牌',
  `p_color` varchar(255) NOT NULL COMMENT '顏色',
  `p_size` varchar(255) NOT NULL COMMENT '尺寸'
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

--
-- 資料表的匯出資料 `prouductcarousel`
--

INSERT INTO `prouductcarousel` (`p_sid`, `p_photo`, `p_name`, `quantity`, `p_description`, `p_price`, `p_genre`, `p_genre2`, `p_brand`, `p_color`, `p_size`) VALUES
